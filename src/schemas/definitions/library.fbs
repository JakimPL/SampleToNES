include "arrays.fbs";
include "instructions/instruction.fbs";
include "configs/library.fbs";
include "metadata.fbs";

namespace schemas.library;

table FBLibraryFragment {
	generator_class: string;
	instruction: schemas.instructions.FBInstruction;
	feature: [float];
	sample: schemas.arrays.FBCyclicArray;
	frequency: float;
}

table FBLibraryData {
	metadata: [schemas.metadata.FBMetadata];
	config: schemas.configs.FBLibraryConfig;
	data: [FBLibraryFragment];
}

root_type FBLibraryData;
